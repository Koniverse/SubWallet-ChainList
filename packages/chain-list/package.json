{
  "name": "@subwallet/chain-list",
  "version": "0.2.122",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./types": {
      "development": "./src/type.ts",
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs",
      "default": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "ts-md5": "^2.0.1"
  },
  "devDependencies": {
    "axios": "^1.11.0",
    "graphql-request": "^7.2.0",
    "tsx": "^4.20.4",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "patch:compare": "tsx ./scripts/utils/compare.ts",
    "patch:data": "tsx ./scripts/patch/patch-data.js",
    "fetch:chains": "tsx ./scripts/strapi/fetch-chains.js",
    "fetch:chain-assets": "tsx ./scripts/strapi/fetch-chain-assets.js",
    "fetch:multi-chain-assets": "tsx ./scripts/strapi/fetch-multi-chain-assets.js",
    "fetch:all": "pnpm fetch:chains && pnpm fetch:chain-assets && pnpm fetch:multi-chain-assets"
  }
}
